
<div class="container">
  <h1></h1>
  <h2>Add a new God</h2>

  <%= simple_form_for(@god) do |f| %>
    <%= f.input :name %>
    <%= f.input :location%>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit %>
  <% end %>
</div>
